<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#DFD5D5"
        tools:context=".Register_11">
    <Button
            android:text="Search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/search" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.058"
            app:layout_constraintVertical_bias="0.014" android:textSize="18sp" android:textStyle="bold"
            android:textColor="#1B1B1A" android:background="#2196F3"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="40dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/fname" android:layout_marginEnd="8dp"
            android:hint="First Name"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintVertical_bias="0.127" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Last Name"
            android:ems="10"
            android:id="@+id/lname" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintVertical_bias="0.236" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="User Name"
            android:ems="10"
            android:id="@+id/uname" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintVertical_bias="0.341" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Address"
            android:ems="10"
            android:id="@+id/address" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintVertical_bias="0.44" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Email"
            android:ems="10"
            android:id="@+id/email" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintVertical_bias="0.546" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Contact Number"
            android:ems="10"
            android:id="@+id/cnum" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintVertical_bias="0.66" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Paaword"
            android:ems="10"
            android:id="@+id/password" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintVertical_bias="0.761" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <EditText
            android:layout_width="290dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Confirm Password"
            android:ems="10"
            android:id="@+id/rpassword" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintVertical_bias="0.865" android:textColor="#2682AC" android:textColorHint="#5A24B9"
            android:textSize="18sp"/>
    <Button
            android:text="Update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/update" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.058"
            app:layout_constraintVertical_bias="1.0" android:textColor="#0C0B0B" android:textSize="18sp"
            android:textStyle="bold" android:background="#8BC34A"/>
    <Button
            android:text="Delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/delete" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.912" app:layout_constraintVertical_bias="1.0"
            android:textSize="18sp" android:textStyle="bold" android:background="#D12116"/>
    <EditText
            android:layout_width="213dp"
            android:layout_height="42dp"
            android:inputType="textPersonName"
            android:hint="NIC "
            android:ems="10"
            android:id="@+id/NIC"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.796" app:layout_constraintVertical_bias="0.012"
            android:textColor="#2682AC" android:textColorHint="#5A24B9" android:textSize="18sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>